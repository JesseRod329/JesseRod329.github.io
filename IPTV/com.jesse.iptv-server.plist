<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.jesse.iptv-server</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/jesse/iptv/backend/start_server.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/jesse/iptv/logs/server.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/jesse/iptv/logs/server.error.log</string>
    <key>WorkingDirectory</key>
    <string>/Users/jesse/iptv/backend</string>
    <key>EnvironmentVariables</key>
    <dict>
        <key>ENVIRONMENT</key>
        <string>dev</string>
        <key>PORT</key>
        <string>5001</string>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
</dict>
</plist>



